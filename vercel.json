{
  "buildCommand": "cd frontend && npm install && npm run build",
  "outputDirectory": "frontend/build",
  "installCommand": "npm install",
  "rewrites": [
    {
      "source": "/api/sessions/(.*)",
      "destination": "/api/sessions"
    },
    {
      "source": "/api/drive/(.*)",
      "destination": "/api/drive"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
} 